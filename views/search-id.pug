extends layout

block content
  div#header
    h1 CHEAT
    h1 STEI ITB 2019
  img#logo(src="../img/logoregis-small-white.png")
  #search-form
    form(action="/search" enctype="multipart/form-data" method="post")
      input(type="file" name="ref")
      input(type="submit" value="Upload")
  div.gallery-stei
    ul#lightgallery
      each image in images
        li(data-src=`../thumb/${image}/25` full=`../images/${image}`)
          a(href)
            img(src=`../thumb/${image}/5`)
